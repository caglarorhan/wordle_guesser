{
  "name": "wordle_guesser",
  "short_name": "Wordle Guesser",
  "description": "Guessing the word extension for Wordle",
  "homepage_url": "https://www.caglarorhan.com",
  "version": "2022.0.2",
  "manifest_version": 3,
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "WORDLE GUESSER",
    "default_icon": "./guess.png"
  },
  "icons": {
    "16": "./guess.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.bundle.app/wordle-tr/"
      ],
      "js": [
        "./js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "./js/content.js"
  ]
}
